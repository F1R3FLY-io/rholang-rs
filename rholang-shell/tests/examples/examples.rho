new ch in { ch!("hello") | for (msg <- ch) { msg } }
new ch in { ch!([1, 2, 3]) | for (data <- ch) { data } }
new outer in {
    new inner in {
        inner!(42) |
        for (x <- inner) { x }
    }
}
// complex example
new channel1, channel2, result in {
    channel1!(42) |
    channel1!("hello") |
    channel2!(true) |
    for (x <- channel1) {
        result!(100)
    } |
    if (1 + 2 == 3) {
        result!([1, 2, 3])
    } else {
        result!(false)
    }
}
[(1 + 1), (2 + 2), (3 + 3)]
((10 - 2) * 3) + 1